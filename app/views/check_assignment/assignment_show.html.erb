<h1>yoooo</h1>
 
 <table width="100%">
  <thead>
    <tr>
      <th>Subject</th>
      <th>Submitted to</th>
      <th>Assignment no</th>
      <th>Attachment</th>
      <th colspan="3"></th>
    </tr>
  </thead>
 <tbody>
    <% @results.each do |f| %>
      <tr>
        <td><%= f.subject %></td>
        <td><%=   f.submitted_to %></td>
        <td><%= f.assignment_no %></td>
        <td><%= link_to'download file', f.attachment.url %></td>
        <td><%= link_to 'Show', f %></td>
        <td><%= link_to 'Edit', edit_assignment_path(assignment) %></td>
        <td><%= link_to 'Destroy', assignment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>

</table>